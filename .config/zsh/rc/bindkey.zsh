# Zsh Key Bindings
bindkey '^g' ghq-fzf                                    # ghq リポジトリジャンプ
bindkey '^p' history-beginning-search-backward-end       # 履歴検索（後方）
bindkey '^n' history-beginning-search-forward-end        # 履歴検索（前方）
bindkey '^[e' edit-command-line                          # Alt+E: エディタでコマンド編集
